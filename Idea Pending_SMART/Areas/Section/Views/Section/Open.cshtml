@model IEnumerable<Enrollment>
@{
    ViewData["Title"] = "EnrollmentID";
    ViewData["Title"] = "ClassTime";
    ViewData["Title"] = "FirstName";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SMART System</title>
    <link href="/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
</head>

<body>
    <div class="bg-dark text-white p-2">
        <span class="navbar-brand ml-2">Your classes</></span>
    </div>

    @foreach (var obj in Model)
    {
       <td>
            <h2>
                Enrollment ID: @obj.EnrollmentID
            </h2>
             <h2> Student ID: @obj.Student.StudentID
             </h2>
            <h2>
               Name: @obj.Student.ApplicationNameFirst
            </h2>
       </td>

    }

</body>
</html>
